<odoo>
  <data>

    <record id="method_simple_api.view_country_form" model="ir.ui.view">
      <field name="name">method.simple.api.base.view.country.form</field>
      <field name="model">res.country</field>
      <field name="inherit_id" ref="base.view_country_form"/>
      <field name="arch" type="xml">      
        <xpath expr="//field[@name='code']" position="after">            
          <field name="code_dte"/>
        </xpath>        
      </field>
    </record>


  </data>
</odoo>